<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="mainPane" prefHeight="750.0" prefWidth="1005.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox prefHeight="490.0" prefWidth="180.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Groups">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </Label>
            <ListView fx:id="groupListView" prefHeight="200.0" prefWidth="200.0" />
            <HBox alignment="CENTER" prefWidth="180.0">
               <children>
                  <Button fx:id="selectAllGroupsBtn" mnemonicParsing="false" text="Select All">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="selectNoneGroupsBtn" mnemonicParsing="false" text="Select None" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
            <VBox alignment="CENTER">
               <children>
                  <Button fx:id="findCalendarBtn" mnemonicParsing="false" text="Find calendar" />
                  <Button fx:id="addAppointmentBtn" mnemonicParsing="false" text="Add appointment">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="calendarNameLabel" prefHeight="27.0" prefWidth="180.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <HBox alignment="BOTTOM_LEFT" prefWidth="290.0">
               <children>
                  <ImageView fx:id="backToCalendarBtn" fitHeight="32.0" fitWidth="32.0" layoutX="10.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../img/calendar.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </HBox>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="221.0">
               <children>
                  <Label fx:id="yearLabel" text="2015">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="prevDateBtn" fitHeight="24.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../img/arrow_left.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="monthLabel" alignment="CENTER" maxWidth="100.0" minWidth="130.0" text="September">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <ImageView fx:id="nextDateBtn" fitHeight="24.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../img/arrow_right.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="BOTTOM_RIGHT" prefWidth="270.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <children>
                  <Pane>
                     <children>
                        <ImageView fx:id="notifyBtn" fitHeight="32.0" fitWidth="32.0" layoutX="110.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../img/bell.png" />
                           </image>
                        </ImageView>
                        <Circle fx:id="notificationCircle" fill="#c2d3ff" layoutX="131.0" layoutY="69.0" radius="8.0" stroke="TRANSPARENT" strokeType="INSIDE" strokeWidth="0.0" />
                        <Label fx:id="notificationLabel" alignment="CENTER" layoutX="124.0" layoutY="60.0" prefWidth="15.0" text="+9" />
                     </children>
                  </Pane>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
