<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gruppe16.main.EmployeeFinder">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="80.0" text="Given name:">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <TextField fx:id="givenNameTextField" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0">
         <children>
            <Label prefWidth="80.0" text="Sir name:">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <TextField fx:id="sirNameTextField" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="employeeListView" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </ListView>
      <ListView fx:id="attendingListView" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
      <HBox>
         <children>
            <Button fx:id="OKBtn" layoutX="221.0" layoutY="10.0" mnemonicParsing="false" text="OK">
               <HBox.margin>
                  <Insets right="140.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="addBtn" mnemonicParsing="false" text="Add">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Button>
            <Button fx:id="removeBtn" mnemonicParsing="false" text="Remove">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
