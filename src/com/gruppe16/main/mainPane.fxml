<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="mainPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox prefHeight="490.0" prefWidth="180.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Groups">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </Label>
            <ListView fx:id="groupListView" prefHeight="200.0" prefWidth="200.0" />
            <HBox alignment="CENTER" prefWidth="180.0">
               <children>
                  <Button fx:id="selectAllGroupsBtn" mnemonicParsing="false" text="Select All">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="selectNoneGroupsBtn" mnemonicParsing="false" text="Select None" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Button fx:id="findCalendarBtn" mnemonicParsing="false" text="Find calendar" />
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="calendarNameLabel" prefHeight="27.0" prefWidth="490.0" text="name">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="221.0">
               <children>
                  <Label fx:id="yearLabel" text="2015">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets right="25.0" />
                     </padding>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="prevDateBtn" mnemonicParsing="false" text="&lt;" />
                        <Label fx:id="monthLabel" alignment="CENTER" maxWidth="100.0" minWidth="100.0" text="September">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Button fx:id="nextDateBtn" mnemonicParsing="false" text="&gt;">
                           <HBox.margin>
                              <Insets left="1.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="backToCalendarBtn" mnemonicParsing="false" text="^" visible="false" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
